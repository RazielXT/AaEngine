material Voxelize
{
	vertex_shader voxelizeVS
	pixel_shader voxelizePS

	culling none
	depth off

	texture Diffuse
	{
		file white.png
	}
	texture ShadowMap0
	texture SceneVoxelBounces
}

material VoxelizeInstancing : Voxelize
{
	vertex_shader voxelizeVSInstancing
	pixel_shader voxelizePSInstancing
}